<div id="boxedWrapper">

<header class="main-header clearfix">
    <div class="container">
        <h1 class="page-title pull-left">index shopping</h1>
        <ol class="breadcrumb pull-right">
            <% if (!member) { %>
            <li><a href="/signup">signup</a></li>
            <li><a href="/reg">registration</a></li>
            <% } else { %>
            <li><%=member.name%><li>
            <li><a href="/logout">Logout</a></li>
            <% } %>
        </ol>
    </div>
</header>


<section class="content-area bg1">
    <div class="container">

        <ul id="galleryFilters" class="option-set clearfix list-unstyled list-inline">
            <li><a href="#filter=*" class="btn btn-default btn-primary">All</a></li>
            <li><a href="#filter=.web" class="btn btn-default">Web</a></li>
            <li><a href="#filter=.identity" class="btn btn-default">Identity</a></li>
            <li><a href="#filter=.print" class="btn btn-default">Print</a></li>
            <li><a href="#filter=.life" class="btn btn-default">Life</a></li>
        </ul>

        <div id="galleryContainer" class="clearfix withSpaces col-3">
            <% if (products) { %>
            <% for ( var i = 0; i < products.length; i++){ %>
            <div class="galleryItem identity">
                <article class="portfolio-item">
                    <div class="portfolio-thumbnail">
                        <a href="/detail/<%= products[i]._id %>"><img src="images/content/works-01.jpg" alt=" "></a>
                        <a href="/detail/<%= products[i]._id %>" class="overlay-img"><span class="overlay-ico"><i class="fa fa-plus"></i></span></a>
                    </div>
                    <div class="entry-meta">
                        <span class="cat-links"><a href="/detail/<%= products[i]._id %>"><%=products[i].name%></a>, <a href="#">link1</a></span>
                    </div>
                    <h4 class="entry-title"><a href="/detail/<%= products[i]._id %>"><%=products[i].description%></a></h4>
                </article>
                <!-- / portfolio-item -->
            </div>
            <% }%>
            <% } %>
            <% if (products) { %>
